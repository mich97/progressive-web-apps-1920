<%- include('partials/head') %>

<section class="main-container">
    <h2>Series</h2>
    <section class="main-overview">
        <% modifiedOverview.forEach(serie => { %>
            <a class="card" href="/m/series/<%= serie.id %>">
                <img class="card-thumbnail" src="<%=  `${serie.img}` %>" alt="Serie Thumbnail">
                <h5 class="card-title"><%= serie.name %></h5>
            </a>
        <% }) %>
    </section>
</section>

<%- include('partials/footer') %>