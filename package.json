    "rimraf": "^3.0.2",
    "npm-run-all": "^4.1.5",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-clean-css": "^4.2.0",
    "gulp-concat": "^2.6.1",
    "dotenv": "^8.2.0"
  },
  "devDependencies": {
    "chokidar-cli": "^2.1.0",
    "nodemon": "^2.0.2"
